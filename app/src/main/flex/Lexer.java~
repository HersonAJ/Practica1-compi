          // fall through
          case 79: break;
          case 35:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "LETRA_BLOQUE");
            }
          // fall through
          case 80: break;
          case 36:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "FIGURA_BLOQUE");
            }
          // fall through
          case 81: break;
          case 37:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "LETRA_SIZE_SI");
            }
          // fall through
          case 82: break;
          case 38:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "COLOR_MIENTRAS");
            }
          // fall through
          case 83: break;
          case 39:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "COLOR_TEXTO_SI");
            }
          // fall through
          case 84: break;
          case 40:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "LETRA_MIENTRAS");
            }
          // fall through
          case 85: break;
          case 41:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "FIGURA_MIENTRAS");
            }
          // fall through
          case 86: break;
          case 42:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "LETRA_SIZE_BLOQUE");
            }
          // fall through
          case 87: break;
          case 43:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "COLOR_TEXTO_BLOQUE");
            }
          // fall through
          case 88: break;
          case 44:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "LETRA_SIZE_MIENTRAS");
            }
          // fall through
          case 89: break;
          case 45:
            { return new Token("CONFIG", yytext(), yyline+1, yycolumn+1, "COLOR_TEXT_MIENTRAS");
            }
          // fall through
          case 90: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }


}
